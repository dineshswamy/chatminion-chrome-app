{
    "name": "Jarviz CalltheTeam 2",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "call your team members with the help of Minion",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
        ]
    },
  "page_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Jarviz CalltheTeam 2",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "styles/main.css"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "pushMessaging",
    "tts",
    "http://*/*",
    "https://*/*"
  ]
}
