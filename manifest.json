{
    "name": "Jarviz CalltheTeam 2",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "call your team members with the help of Minion",
    "icons": {
        "16": "images/jarviz_background-16.png",
        "48": "images/jarviz_background-48.png",
        "128": "images/jarviz_background-120.png"
    },
    "default_locale": "en",
    "background": {
      "page":"background.html"

    },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Jarviz CalltheTeam 2"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "styles/main.css"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "pushMessaging",
    "tts",
    "http://localhost:3000/*",
    "https://www.google.com/accounts/OAuthGetRequestToken",
    "https://www.google.com/accounts/OAuthAuthorizeToken",
    "https://www.google.com/accounts/OAuthGetAccessToken",
    "https://www.googleapis.com/auth/userinfo#email"
  ]
}
